<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.sf.eclipsensis"
      label="%feature.name"
      version="0.9.0"
      provider-name="%provider.name"
      os="win32"
      ws="win32"
      arch="x86">

   <install-handler/>

   <description url="%description.url">
      %description.text
   </description>

   <copyright url="%copyright.url">
      %copyright.text
   </copyright>

   <license url="%license.url">
      %license.text
   </license>

   <url>
      <update label="%update.site.name" url="http://eclipsensis.sf.net/update/"/>
      <discovery label="%discovery.site.name" url="http://eclipsensis.sf.net/update/"/>
   </url>

   <includes
         id="net.sf.eclipsensis.core"
         version="0.9.0"
         name="%feature.name"/>

   <includes
         id="net.sf.eclipsensis.installoptions"
         version="0.9.0"
         name="%feature.name"
         optional="true"/>

</feature>
